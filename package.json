{
  "name": "autone-prototypes",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "dev:frontend": "turbo dev --filter=@autone/frontend",
    "dev:backend": "turbo dev --filter=@autone/backend",
    "build": "turbo build",
    "build:frontend": "turbo build --filter=@autone/frontend",
    "build:backend": "turbo build --filter=@autone/backend",
    "storybook": "pnpm --filter @autone/frontend storybook",
    "db:migrate": "pnpm --filter @autone/backend db:migrate",
    "db:push": "pnpm --filter @autone/backend db:push",
    "db:studio": "pnpm --filter @autone/backend db:studio",
    "db:generate": "pnpm --filter @autone/backend db:generate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.2.5",
    "turbo": "^2.3.3",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.21.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.4"
}
